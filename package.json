{
  "name": "@cao-mei-you-ren/koishi-plugin-adapter-wechaty",
  "version": "1.0.0",
  "description": "Koishi Wechaty 适配器",
  "author": "CaoMeiYouRen",
  "private": false,
  "license": "AGPL-3.0",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint src --fix --ext .ts,.js",
    "release": "semantic-release",
    "build": "rimraf dist && tsc",
    "dev": "cross-env NODE_ENV=development ts-node-dev src/index.ts",
    "rm": "rimraf node_modules",
    "watch": "tsc -w",
    "start": "node ./dist/index",
    "commit": "cz",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^15.0.0",
    "@satorijs/protocol": "^1.1.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.10.3",
    "@types/qrcode": "^1",
    "@types/qrcode-terminal": "^0",
    "@types/raven": "^2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "commitizen": "^4.2.3",
    "conventional-changelog-cmyr-config": "^2.1.1",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog-cmyr": "^1.0.0",
    "debug": "^4.3.4",
    "eslint": "^8.50.0",
    "eslint-config-cmyr": "^1.1.30",
    "eslint-plugin-import": "^2.28.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.1.2",
    "raven": "^2.6.4",
    "rimraf": "^5.0.5",
    "semantic-release": "21.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "file-box": "1.4.15",
    "file-type": "16.5.4",
    "mime2ext": "^1.0.1",
    "qrcode": "^1.5.3",
    "qrcode-terminal": "^0.12.0",
    "tslib": "^2.6.2",
    "wechaty": "^1.20.2",
    "wechaty-puppet-wechat": "^1.18.4"
  },
  "peerDependencies": {
    "koishi": "^4.15.7"
  },
  "keywords": [
    "chatbot",
    "koishi",
    "plugin",
    "wechat",
    "wechaty"
  ],
  "engines": {
    "node": ">=16"
  },
  "homepage": "https://github.com/CaoMeiYouRen/koishi-plugin-adapter-wechaty#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaoMeiYouRen/koishi-plugin-adapter-wechaty.git"
  },
  "bugs": {
    "url": "https://github.com/CaoMeiYouRen/koishi-plugin-adapter-wechaty/issues"
  },
  "changelog": {
    "language": "zh"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-cmyr"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "koishi": {
    "category": "adapter",
    "service": {
      "implements": [
        "adapter"
      ]
    }
  }
}
